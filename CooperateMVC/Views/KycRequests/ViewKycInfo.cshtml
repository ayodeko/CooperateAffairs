@model KycInfo
@{
}

<div>
    <h4>Kyc Info</h4>
    <dl>
        <dt>
            @Html.DisplayNameFor(model => model.RCNumber)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.RCNumber)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CompanyName)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CompanyName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CompanyObjective)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CompanyObjective)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.CompanyOfficersList)
        </dt>
            @for (int i = 0; i < Model.CompanyOfficersList.Count; i++)
                {
                    <dd>
                        @{var hol = $"{i + 1}. - {Model.CompanyOfficersList[i].Name}";}
                        @{var id = i; Console.WriteLine($"IDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD: {id}");}
                        @hol
                    </dd>
                }
        <dt>
            @Html.DisplayNameFor(model => model.ShareHolders)
        </dt>
            @for (int i = 0; i < Model.ShareHolders.Count; i++)
                {
                    <dd>
                        @{var hol = $"{i + 1}. - {Model.ShareHolders[i].Name}";}
                        @{var id = i; Console.WriteLine($"IDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD: {id}");}
                        @hol
                    </dd>
                }

    </dl>

</div>